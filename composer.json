{
    "name": "tracekit/laravel-apm",
    "description": "TraceKit APM for Laravel - Zero-config distributed tracing and performance monitoring",
    "keywords": ["laravel", "apm", "tracing", "monitoring", "opentelemetry", "performance"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TraceKit",
            "email": "support@tracekit.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "open-telemetry/sdk": "^1.0",
        "open-telemetry/exporter-otlp": "^1.0",
        "open-telemetry/api": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "TraceKit\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TraceKit\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TraceKit\\Laravel\\TracekitServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
